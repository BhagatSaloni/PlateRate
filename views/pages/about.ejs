<!-- views/pages/about.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
    
</header>
<ol>
    

    <% abt.forEach(function(ab) { %>
        <li><font size='12px' color= '#3cb371'</font><%= ab.description %> </li>
        <li><font size='16px' color= '#4A4A4A.'</font><%= ab.tech %></li>
        <li><font size='16px' color= '#4A4A4A.'</font><%= ab.techstack %></li>
        <li><font size='16px' color= '#4A4A4A.'</font><%= ab.hobbies %></li>
    <% }); %>
  
    </ol>

<main>


<div class="row">
    <div class="col-sm-8">    
        <div class="jumbotron">
            <h1>This is great</h1>
            <p>Welcome to templating using EJS</p>
        </div>
    </div>

    <div class="col-sm-4">        
        <div class="well">
            <h3>Look I'm A Sidebar!</h3>
        </div>
    </div>

</div>
</main>

<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>
